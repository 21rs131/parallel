{
    "name": "parallel-python",
    "build": {
        "dockerfile": "./Dockerfile"
    },
    "workspaceFolder": "/workspace",
    "workspaceMount": "type=bind,source=${localWorkspaceFolder},target=/workspace,consistency=cached",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "oderwat.indent-rainbow"
            ]
        }
    }
}